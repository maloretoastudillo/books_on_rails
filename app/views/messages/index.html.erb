<h1>Casilla de mensajes</h1>


<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Remitente</th>
      <th scope="col">Asunto</th>
      <th scope="col">Recibido el</th>
    </tr>
  </thead>
  <tbody>
    <% @received.messages.order.each do |message| %>
    <tr>
      <th scope="row"><%= message. %></th>
      <td>Mark</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>
      <td>@fat</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td colspan="2">Larry the Bird</td>
      <td>@twitter</td>
    </tr>
  </tbody>
</table>